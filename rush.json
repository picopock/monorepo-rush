{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.56.0",
  "yarnVersion": "1.22.10",
  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "approvedPackagesPolicy": {
    "reviewCategories": [
      "internal"
    ],
    "ignoredNpmScopes": [
      "@types"
    ]
  },
  "gitPolicy": {
    "allowedEmailRegExps": [
      "[^@]+@users\\.noreply\\.github\\.com",
      ".*@163\\.com"
    ],
    "sampleEmail": "example@163.com"
  },
  "repository": {
    "url": "https://github.com/xinsui01/monorepo-rush",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "@demo/cli",
      "projectFolder": "backend/cli",
      "reviewCategory": "internal"
    },
    {
      "packageName": "@demo/webim",
      "projectFolder": "backend/im",
      "reviewCategory": "internal"
    },
    {
      "packageName": "@demo/koa2-templete",
      "projectFolder": "backend/koa2_templete",
      "reviewCategory": "internal"
    },
    {
      "packageName": "@demo/react-feature",
      "projectFolder": "frontend/react_feature",
      "reviewCategory": "internal"
    },
    {
      "packageName": "@demo/svelte",
      "projectFolder": "frontend/svelte",
      "reviewCategory": "internal"
    },
    {
      "packageName": "@demo/vue3",
      "projectFolder": "frontend/vue3",
      "reviewCategory": "internal"
    }
  ]
}